{
  "name": "@fastpanel/cli",
  "version": "0.0.1-alpha.1",
  "description": "Development tools for \"fastPanel\".",
  "author": "Desionlab <fenixphp@gmail.com> (http://www.desionlab.net/)",
  "preferGlobal": true,
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "compile": "./node_modules/.bin/tsc",
    "develop": "./node_modules/.bin/tsc --watch",
    "test": "./node_modules/.bin/mocha --require ts-node/register test/**/*.spec.ts",
    "build": "npm run test && npm run compile",
    "prepare": "npm run build"
  },
  "bin": {
    "fpm": "./bin/fpm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastpanel/cli.git"
  },
  "bugs": {
    "url": "https://github.com/fastpanel/cli/issues"
  },
  "homepage": "https://github.com/fastpanel/cli#readme",
  "keywords": [
    "nodejs",
    "typescript",
    "cli",
    "fastPanel"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@fastpanel/core": "^0.1.1-alpha.2",
    "ejs": "^2.6.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/ejs": "^2.6.3",
    "@types/inquirer": "^6.0.1",
    "@types/lodash": "^4.14.126",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "@types/prettyjson": "^0.0.29",
    "@types/progress": "^2.0.3",
    "@types/shelljs": "^0.8.5",
    "@types/triple-beam": "^1.3.0",
    "@types/validator": "^10.11.0",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "ts-node": "^8.1.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5"
  },
  "license": "MIT"
}
